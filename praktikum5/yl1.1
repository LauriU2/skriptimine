#!/bin/bash
#
echo -n "Sisesta kolmurga esimene külg cm-ides: "
read a

echo -n "Sisesta kolmurga teine külg cm-ides: "
read b

hypotenuus=$(echo $a^2+$b^2 | bc -l ) #arvutab kahe külje korrutise
calc=$(echo "scale=2; sqrt($hypotenuus)" | bc -l ) #arvutab kolmanda külje ruutjuure
echo "Kolmnurga hüpotenuus on: $calc cm"

